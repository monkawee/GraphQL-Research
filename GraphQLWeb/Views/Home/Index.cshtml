@model List<GraphQLWeb.Models.UserInfo>

@{
    ViewData["Title"] = "Home Page";
}

@{
    if (Model != null || Model?.Count > 0)
    {
        <div>
            @foreach (var user in Model)
            {
                <div class="d-flex justify-content-center align-items-center">
                    <div class="mx-auto">
                        UserID: @user.UserID, User Name: @user.UserName, Password: @user.Password, Citizen ID: @user.CitizenID, E-mail: @user.Email, Mobile No.: @user.MobileNo, Address: @user.Address, Line of Command: @user.LineOfCommand
                    </div>                    
                </div>
            }
        </div>
    }    
}

@section Scripts {
    <script>

    </script>
}

